package com.sam;

public class no_of_chars {

	public static void main(String[] args) {
       String str="swarupa1234";
       int count=0;
       for(int i=0;i<str.length();i++)
       {
    	   if(str.charAt(i)!=' ')
    		   count++;
    		   
    	   
       }
     System.out.println(count);
	}

}
